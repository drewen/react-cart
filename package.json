{
  "name": "react-cart",
  "version": "0.2.0",
  "description": "Javascript cart handler for shopping websites",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/drewen/react-cart.git"
  },
  "scripts": {
    "prepublish": "babel src/ --out-dir dist",
    "test": "NODE_ENV=test nyc mocha",
    "lint": "eslint src test"
  },
  "keywords": [
    "javascript",
    "react-cart",
    "react"
  ],
  "author": "Andrew Smith",
  "license": "MIT",
  "dependencies": {
    "lodash": "4.16.4",
    "react-redux": "4.4.5",
    "redux": "3.6.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-plugin-istanbul": "4.1.5",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "coveralls": "2.11.14",
    "enzyme": "2.4.1",
    "eslint": "4.8.0",
    "eslint-plugin-mocha": "4.11.0",
    "eslint-plugin-react": "7.4.0",
    "jsdom": "9.6.0",
    "jsdom-global": "2.1.0",
    "mocha": "5.0.0",
    "nyc": "11.4.1",
    "react": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2"
  },
  "peerDependencies": {
    "react": ">=14.0.0",
    "react-dom": ">=14.0.0"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false,
    "extension": [
      ".jsx",
      ".js"
    ],
    "include": "src",
    "all": true
  },
  "babel": {
    "presets": ["es2015", "react", "env"],
    "env": {
      "test": {
        "plugins": ["istanbul"]
      }
    }
  }
}
